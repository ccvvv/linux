#!/usr/bin/env bash
TMOE_BIN='/opt/picgo/app.asar'
ELECTRON_BIN='/opt/electron/electron'
export ELECTRON_IS_DEV=0
case "$(id -u)" in
0) ${ELECTRON_BIN} ${TMOE_BIN} --no-sandbox "$@" ;;
*)
    ${ELECTRON_BIN} ${TMOE_BIN} "$@"
    case "$?" in
    0) ;;
    *) ${ELECTRON_BIN} ${TMOE_BIN} --no-sandbox "$@" ;;
    esac
    ;;
esac
